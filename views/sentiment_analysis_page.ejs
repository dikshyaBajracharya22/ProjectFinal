<body>
    <%- include("./partial/menu.ejs")%>

    <div class="thanks-content">
        <p><%= review%></p>
        <br>
        <h1> Thanks for giving your precious review !!</h1>
    
      <br>
        
        <img src="image/<%=rate%>.png" height="120px" width="120px"/>
        <br>
<div>
    <%if (rate==1){%>
        <p>Your review was poor ...</p>
    <%}else if(rate==2){%>
        <p>Your review was poor ...</p>
    <%}else if(rate==3){%>
        <p>Your review was average ...</p>
